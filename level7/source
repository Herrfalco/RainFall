#include <time.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>

char	c[68];

void	m(void) {
	printf("%s - %d\n", c, (int)time(NULL));
}

int		main(int argc, char **argv) {
	unsigned int	*a;
	unsigned int	*b;

	b = malloc(8);
	b[0] = 1;
	b[1] = (unsigned int)malloc(8);

	a = malloc(8);
	a[0] = 2;
	a[1] = (unsigned int)malloc(8);

	strcpy((char *)b[1], argv[1]);
	strcpy((char *)a[1], argv[2]);

	fgets(c, 68, fopen("/home/user/level8/.pass", "r"));
	puts("~~");

	return (0);
}
